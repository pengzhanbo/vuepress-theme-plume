{
  "extends": "tsconfig-vuepress/base.json",
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["DOM", "ES2024"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "isolatedDeclarations": true,
    "skipLibCheck": true
  }
}
